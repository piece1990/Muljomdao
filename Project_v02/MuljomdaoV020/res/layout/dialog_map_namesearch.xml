<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

	<TextView
	    android:id="@+id/dialog_map_search_addr"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:ems="10"
	    android:textIsSelectable="false" />
	<LinearLayout
		    android:id="@+id/foodserachlayout"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal" >
		<ImageView
                android:id="@+id/fs_button1"
                android:layout_width="75dp"
                android:layout_height="60dp"
                android:src="@drawable/di_fish" />
		
		<ImageView
                android:id="@+id/fs_button2"
                android:layout_width="75dp"
                android:layout_height="60dp"
                android:src="@drawable/di_nakgi" />
		<ImageView
                android:id="@+id/fs_button3"
                android:layout_width="75dp"
                android:layout_height="60dp"
                android:src="@drawable/di_bajirook" />
		<ImageView
                android:id="@+id/fs_button4"
                android:layout_width="75dp"
                android:layout_height="60dp"
                android:src="@drawable/di_cafe" />
	</LinearLayout>
</LinearLayout>
